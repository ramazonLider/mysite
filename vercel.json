{
  "builds": [
    {
      "src": "mysite/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/ws/(.*)",
      "dest": "mysite/asgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "mysite/asgi.py"
    }
  ],
  "scripts": {
    "start": "daphne -b 0.0.0.0 -p 8000 mysite.asgi:application"
  }
}
